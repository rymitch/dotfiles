[color]
	status = auto
	branch = auto
[alias]
  lg = log --graph --format=format:'%C(blue)%h%C(reset) - %C(cyan)%cD%C(reset) %C(green)(%cr)%C(reset)%C(yellow)%d%C(reset)%n''          %C(black)%s%C(reset) %C(magenta)â€” %ae%C(reset)' --abbrev-commit
  la = !"git lg --all ; echo"
  l = !"git --no-pager lg --max-count=25 ; echo"
  dm = !"git diffall"
  st = status -sb
[diff]
  tool = diffmerge
[difftool "diffmerge"]
	cmd = diffmerge $LOCAL $REMOTE
